		timeToLiveSeconds="36000" eternal="false" memoryStoreEvictionPolicy="LRU"
		overflowToDisk="false">
		<cacheEventListenerFactory
			class="com.baidu.stock.process.cache.mannage.ProcessorCacheEventListenerFactory" />
	</cache>

